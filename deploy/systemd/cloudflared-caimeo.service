[Unit]
Description=Cloudflare Tunnel for CAIMEO
After=network-online.target
Wants=network-online.target

[Service]
User=pi
WorkingDirectory=/home/pi
ExecStart=/usr/bin/cloudflared --config /home/pi/CaimeoV1/deploy/cloudflared/config.yml tunnel run
Restart=always
RestartSec=5

[Install]
WantedBy=multi-user.target
